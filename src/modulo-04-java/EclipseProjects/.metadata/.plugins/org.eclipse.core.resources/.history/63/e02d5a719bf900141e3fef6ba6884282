package filmator;

import java.util.ArrayList;

public class Filme {
	private ArrayList<Ator> atores;
	
	
	public String toString(){
		String elenco = "";
		for (Ator ator : atores) {
			 elenco += ator.getNome() +"\n";
		}
		return elenco;
	}
	
	public void insereAtor(Ator ator){
		atores.add(ator);
	}
}
